[Unit]
Description=Autok8s Cloud Controller Manager
Documentation=https://github.com/kubernetes/kubernetes

[Service]
ExecStart=/usr/local/bin/cloud-controller-manager \
  --cloud-provider=autok8s \
  --v=5 \
  --kubeconfig /root/.kube/config

Restart=on-failure
RestartSec=5


[Install]
WantedBy=multi-user.target
